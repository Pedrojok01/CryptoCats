{
    "name": "cryptocats-nextjs",
    "version": "2.0.0",
    "description": "Crypto-Cats - Create, Breed, and Sell your Crypto-Cats!",
    "author": "Pedrojok01 <pedrojok@pm.me>",
    "license": "MIT",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint --fix",
        "prettier:js": "prettier --write \"**/*.{ts,tsx,js}\"",
        "prettier:sol": "prettier --write \"**/*.sol\"",
        "prettier": "run-s prettier:js prettier:sol",
        "compile": "hardhat compile --show-stack-traces",
        "deploy": "hardhat run --network goerli hardhat/scripts/deploy.cts",
        "size": "hardhat size-contracts",
        "doc": "hardhat docgen",
        "cover": "hardhat coverage",
        "slither": "slither ./hardhat/contracts --solc-remaps @openzeppelin=node_modules/@openzeppelin --exclude naming-convention,external-function,low-level-calls --buidler-ignore-compile"
    },
    "dependencies": {
        "@chakra-ui/icons": "2.0.13",
        "@chakra-ui/react": "2.4.3",
        "@emotion/react": "11.10.5",
        "@emotion/styled": "11.10.5",
        "@netlify/plugin-nextjs": "^4.29.3",
        "ethers": "5.7.2",
        "framer-motion": "7.9.1",
        "next": "13.0.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-jazzicon": "1.0.4",
        "wagmi": "~0.9.3"
    },
    "devDependencies": {
        "@ethersproject/abi": "5.7.0",
        "@ethersproject/providers": "5.7.2",
        "@nomicfoundation/hardhat-chai-matchers": "1.0.5",
        "@nomicfoundation/hardhat-network-helpers": "1.0.7",
        "@nomicfoundation/hardhat-toolbox": "2.0.0",
        "@nomiclabs/hardhat-ethers": "2.2.1",
        "@nomiclabs/hardhat-etherscan": "3.1.3",
        "@typechain/ethers-v5": "10.2.0",
        "@typechain/hardhat": "6.1.5",
        "@types/chai": "4.3.4",
        "@types/jquery": "3.5.14",
        "@types/mocha": "10.0.1",
        "@types/node": "18.11.15",
        "@types/react": "18.0.9",
        "@types/react-blockies": "1.4.1",
        "@types/react-dom": "18.0.3",
        "@typescript-eslint/eslint-plugin": "5.46.1",
        "@typescript-eslint/parser": "5.46.1",
        "chai": "4.3.7",
        "dotenv": "16.0.3",
        "eslint": "8.15.0",
        "eslint-config-next": "13.0.6",
        "eslint-config-prettier": "8.5.0",
        "eslint-import-resolver-typescript": "3.5.2",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-prettier": "4.2.1",
        "fs": "0.0.1-security",
        "hardhat": "2.12.0-esm.1",
        "hardhat-contract-sizer": "2.6.1",
        "hardhat-docgen": "1.3.0",
        "hardhat-gas-reporter": "1.0.9",
        "npm-run-all": "4.1.5",
        "prettier": "2.8.1",
        "prettier-plugin-solidity": "1.1.0",
        "solhint": "3.3.7",
        "solhint-plugin-prettier": "0.0.5",
        "solidity-coverage": "0.8.2",
        "ts-node": "10.9.1",
        "typechain": "8.1.1",
        "typescript": "4.9.4"
    }
}
